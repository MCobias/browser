{
  "name": "brick/browser",
  "description": "PHP browser implementation for automated application testing",
  "type": "library",
  "keywords": [
    "Brick",
    "Browser"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.1 || ^8.0",
    "brick/http": "~0.2.0",
    "brick/std": "~0.1.0",
    "symfony/css-selector": "^2.8 || ^3.0",
    "ext-dom": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5 || ^8.0 || ^9.0",
    "php-coveralls/php-coveralls": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Brick\\Browser\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Brick\\Browser\\Tests\\": "tests/"
    }
  }
}
