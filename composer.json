{
  "name": "brick/browser",
  "description": "PHP browser implementation for automated application testing",
  "type": "library",
  "keywords": [
    "Brick",
    "Browser"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1",
    "brick/http": "0.2.*",
    "brick/std": "0.1.*",
    "symfony/css-selector": "2.8.* || 3.*"
  },
  "require-dev": {
    "phpunit/phpunit": "6.*",
    "satooshi/php-coveralls": "1.*"
  },
  "autoload": {
    "psr-4": {
      "Brick\\Browser\\": "src/",
      "Brick\\Browser\\Tests\\": "tests/"
    }
  }
}